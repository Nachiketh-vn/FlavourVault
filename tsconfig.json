{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/api/auth/[...nextauth]/route.ts",
    "app/Providers.js",
    "app/login/page.jsx",
    "app/register/page.jsx",
    "app/userRestaurant/[id]/page.jsx",
    "app/api/menu/route.js",
    "models/menu.js",
    "app/user/menu/[id]/page.jsx",
    "app/addReview/[id]/page.jsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
